<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>facebookSignUp</servlet-name>
  	<display-name>facebookSignUp</display-name>
  	<description></description>
  	<servlet-class>com.facebook.web.facebookSignUp</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>faceBookLogIn</servlet-name>
  	<display-name>faceBookLogIn</display-name>
  	<description></description>
  	<servlet-class>com.facebook.web.faceBookLogIn</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>facebookSignUp</servlet-name>
  	<url-pattern>/facebookSignUp</url-pattern>
  </servlet-mapping>
  
  
  <servlet-mapping>
  	<servlet-name>faceBookLogIn</servlet-name>
  	<url-pattern>/faceBookLogIn</url-pattern>
  </servlet-mapping>
</web-app>
